        <choice location="random" name="foil1" correct="true">
			<p>k1&gt; k2&gt; k3</p>
        </choice>
        <choice location="random" name="foil2" correct="false">
			<p>k3&gt; k2&gt; k1</p>
        </choice>
        <choice location="random" name="foil13" correct="false">
			<p>k1= k3&gt; k2</p>
        </choice>
        <choice location="random" name="foil1\4" correct="false">
			<p>k2&gt; k1= k3</p>
        </choice>
        <choice location="random" name="foil5" correct="false">
			<p>k1&gt; k3&gt; k2</p>
        </choice>
      </choicegroup>
    </multiplechoiceresponse>
